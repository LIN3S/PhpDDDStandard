{
  "name": "drupal-composer/drupal-project",
  "description": "Project template for Drupal 8 projects with composer",
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "composer",
      "url": "https://packages.lin3s.com"
    }
  ],
  "require": {
    "advanced-custom-fields-pro": "^5.6",
    "composer/installers": "^1.2",
    "cweagans/composer-patches": "^1.6",
    "johnpbloch/wordpress": "^4.8",
    "wpackagist-plugin/acf-to-rest-api": "^2.2",
    "wpackagist-plugin/wordpress-seo": "^5.5",
    "wpackagist-theme/twentysixteen": "^1.3"
  },
  "extra": {
    "wordpress-install-dir": "core",
    "installer-paths": {
      "plugins/{$name}/": ["type:wordpress-plugin"],
      "themes/{$name}/": ["type:wordpress-theme"],
      "mu-plugins/{$name}/": ["type:wordpress-muplugin"]
    }
  }
}
